{
  "private": true,
  "name": "@dank/opentelemetry",
  "version": "0.0.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./dankInstrumentation": {
      "types": "./dist/instrumentation.d.ts",
      "default": "./dist/instrumentation.js"
    },
    "./dankInstrumentationNode": {
      "types": "./dist/instrumentation.node.d.ts",
      "default": "./dist/instrumentation.node.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "typecheck": "tsc"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.1",
    "@dank/tsconfig": "workspace:^",
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.51.1",
    "@opentelemetry/resources": "^1.24.1",
    "@opentelemetry/sdk-node": "^0.51.1",
    "@opentelemetry/sdk-trace-node": "^1.24.1",
    "@opentelemetry/semantic-conventions": "^1.24.1",
    "tsup": "^8.0.2"
  },
  "dependencies": {
    "ajv": "^8.14.0"
  }
}
